pipeline {
    agent any
    options {
        buildDiscarder(logRotator(numToKeepStr: '5'))
    }
    stages {
        stage('Scan') {
            steps {
                script {
                    // SonarQube analysis for JavaScript/React code
                    withSonarQubeEnv(installationName: 'sq1') {
                        sh 'sonar-rithy'
                    }
                }
            }
        }
    }
}
